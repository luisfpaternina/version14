<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_form_bim_project_project_users" model="ir.ui.view">
            <field name="name">view.form.bim.project.project.users</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[2]/group[1]" position="after">
                    <group>
                        <field name="project_allowed_ids" widget="many2many_tags"/>
                    </group>
                </xpath>
            </field>
        </record>

    </data>
</odoo>